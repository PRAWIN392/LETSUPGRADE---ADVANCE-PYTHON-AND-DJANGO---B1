    "import time\n",
    "utc_time=\"15-08-2020 15:20:20\"\n",
    "def humanToEpoch(utc_time):\n",
    "    epochconvert=time.strptime(utc_time,\"%d-%m-%Y %H:%M:%S\")\n",
    "    epoch_Time=time.mktime(epochconvert)\n",
    "    return epoch_Time\n",
    "print(humanToEpoch(utc_time))"
